{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mentoring-adf-sy"
		},
		"LS_azurementoringausy_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_azurementoringausy'"
		},
		"azurementoringdatalakesy_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'azurementoringdatalakesy'"
		},
		"AzureKeyVaultSY_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://mentoring-key-vault-sy.vault.azure.net/"
		},
		"AzureSqlDatabase1_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "secretdb-con"
		},
		"LS_DB_DataMart_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "secretDataMart"
		},
		"LS_Local_Server_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "secretloc-db"
		},
		"LS_azurekeydb_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://KeyValue-db.vault.azure.net/"
		},
		"LS_online_container_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'LS_online_container'"
		},
		"azurementoringdatalakesy_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azurementoringdatalakesy.dfs.core.windows.net"
		},
		"azurementoringstoragesy_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'azurementoringstoragesy'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "mentoring-adf-sy_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_azurementoringausy_connectionString": {
						"value": "[parameters('LS_azurementoringausy_connectionString')]"
					},
					"azurementoringdatalakesy_accountKey": {
						"value": "[parameters('azurementoringdatalakesy_accountKey')]"
					},
					"AzureKeyVaultSY_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVaultSY_properties_typeProperties_baseUrl')]"
					},
					"LS_azurekeydb_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_azurekeydb_properties_typeProperties_baseUrl')]"
					},
					"LS_online_container_sasUri": {
						"value": "[parameters('LS_online_container_sasUri')]"
					},
					"azurementoringdatalakesy_properties_typeProperties_url": {
						"value": "[parameters('azurementoringdatalakesy_properties_typeProperties_url')]"
					},
					"azurementoringstoragesy_sasUri": {
						"value": "[parameters('azurementoringstoragesy_sasUri')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "mentoring-adf-sy_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_connectionString_secretName')]"
					},
					"LS_DB_DataMart_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_DB_DataMart_properties_typeProperties_connectionString_secretName')]"
					},
					"LS_Local_Server_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_Local_Server_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'mentoring-adf-sy_ArmTemplate_0')]"
			]
		},
		{
			"name": "mentoring-adf-sy_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'mentoring-adf-sy_ArmTemplate_1')]"
			]
		}
	]
}