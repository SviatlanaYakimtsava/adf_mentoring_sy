								"storedProcedureName": "[etl].[GetErrorHandling]",
								"storedProcedureParameters": {
									"ActivityName": {
										"value": {
											"value": "@concat('CopyData from ',item().name)",
											"type": "Expression"
										},
										"type": "String"
