									"DateError": {
										"value": {
											"value": "@{utcnow()}",
											"type": "Expression"
										},
										"type": "DateTime"
									},
									"ErrorCode": {
										"value": {
											"value": "@{activity('CopyData').Output.errors[0].Code}",
											"type": "Expression"
										},
										"type": "String"
									},
									"ErrorMessage": {
										"value": {
											"value": "@{activity('CopyData').output.errors[0].Message}",
											"type": "Expression"
										},
										"type": "String"
